<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yxy.resume.mapper.ResumeMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.yxy.resume.pojo.Resume">
        <id column="id" property="id" />
        <result column="name" property="name" />
        <result column="gender" property="gender" />
        <result column="brith_place" property="brithPlace" />
        <result column="birthday" property="birthday" />
        <result column="age" property="age" />
        <result column="polit_status" property="politStatus" />
        <result column="cellphone" property="cellphone" />
        <result column="academic_career" property="academicCareer" />
        <result column="address" property="address" />
        <result column="work_experience" property="workExperience" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, name, gender, brith_place, birthday, age, polit_status, cellphone, academic_career, address, work_experience
    </sql>

</mapper>
